# Gateway env example for HeartMula on ada2
# Point the gateway to the HeartMula service running on ada2
HEARTMULA_BASE_URL=http://ada2:9920
HEARTMULA_TIMEOUT_SEC=600
# Ensure MUSIC_BACKEND_CLASS is set to heartmula_music in gateway config/backends_config.yaml or via env
MUSIC_BACKEND_CLASS=heartmula_music
